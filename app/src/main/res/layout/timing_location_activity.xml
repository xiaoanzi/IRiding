<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
<com.baidu.mapapi.map.MapView
        android:id="@+id/mv_timing_bmapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true" />
    <ImageButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="16dp"
        android:src="@drawable/abc_ic_menu_copy_mtrl_am_alpha"
        android:background="@drawable/shape"
        android:layout_above="@+id/ib_timing_myloc"
        android:id="@+id/ib_timing_switching"/>
    <ImageButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="16dp"
        android:src="@drawable/ic_radio_button_on_white_24dp"
        android:background="@drawable/shape"
        android:layout_alignBottom="@+id/mv_timing_bmapView"
        android:id="@+id/ib_timing_myloc"/>
    <ImageButton
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:src="@drawable/ic_location_on_white_36dp"
        android:background="@drawable/shape"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="16dp"
        android:layout_alignParentRight="true"
        android:layout_alignBottom="@+id/mv_timing_bmapView"
        android:id="@+id/ib_timing_location2"/>
</merge>
